<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config>
    <printer>
        <hostname>cgaptestbc</hostname>
        <labelType>tiny</labelType>
        <printerType>squix</printerType>
    </printer>

    <printer>
        <hostname>slidelabel</hostname>
        <labelType>slide</labelType>
        <printerType>squix</printerType>
    </printer>

    <printer>
        <hostname>stub</hostname>
        <labelType>thin</labelType>
        <printerType>stub</printerType>
    </printer>

    <printer_type>
        <ftpCredentials>
            <password>ftp_password</password>
            <username>ftp_username</username>
        </ftpCredentials>
        <soapCredentials>
            <password>soap_password</password>
            <username>soap_username</username>
        </soapCredentials>
        <language>JSCRIPT</language>
        <name>squix</name>
        <protocol>FTP</protocol>
        <statusProtocol>SOAP</statusProtocol>
    </printer_type>

    <printer_type>
        <name>stub</name>
        <language>JSCRIPT</language>
        <protocol>STUB</protocol>
    </printer_type>


    <label_type>
        <displacement>12</displacement>
        <height>9</height>
        <name>tiny</name>
        <width>28</width>
    </label_type>

    <label_type>
        <displacement>11</displacement>
        <height>8</height>
        <width>90</width>
        <name>thin</name>
    </label_type>

    <label_type>
        <displacement>22</displacement>
        <height>19</height>
        <width>23</width>
        <name>slide</name>
    </label_type>
</config>
